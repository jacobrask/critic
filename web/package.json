{
  "devDependencies": {
    "jest-cli": "~0.4"
  },
  "jest": {
    "setupEnvScriptFile": "<rootDir>/jest-environment.js",
    "testPathDirs": [
      "<rootDir>/src"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/src/deps/"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/src/constants.js",
      "<rootDir>/src/deps/react.js"
    ]
  }
}
